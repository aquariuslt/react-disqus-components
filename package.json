{
  "name": "react-disqus-components",
  "version": "1.2.3",
  "description": "react-disqus-components wrapper for official disqus web api",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "Aquariuslt <superaquariuslt@gmail.com>",
  "homepage": "https://github.com/aquariuslt/react-disqus-components",
  "repository": "https://github.com/aquariuslt/react-disqus-components",
  "license": "MIT",
  "keywords": [
    "disqus",
    "react",
    "functional-components"
  ],
  "scripts": {
    "build": "tsc && rollup -c rollup.config.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:cov": "jest --coverage",
    "release": "semantic-release"
  },
  "files": [
    "dist",
    "index.ts"
  ],
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "26.0.13",
    "@types/node": "12.12.31",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.4.2",
    "prettier": "2.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "rollup": "2.26.9",
    "rollup-plugin-typescript": "1.0.1",
    "semantic-release": "17.1.1",
    "ts-jest": "26.3.0",
    "tslib": "2.0.1",
    "typescript": "4.0.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint"
    ],
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-member-accessibility": "off",
      "@typescript-eslint/no-parameter-properties": "off",
      "@typescript-eslint/interface-name-prefix": "off",
      "@typescript-eslint/no-var-requires": "off"
    }
  },
  "prettier": {
    "semi": true,
    "arrowParens": "always",
    "proseWrap": "never",
    "singleQuote": true,
    "printWidth": 120
  },
  "jest": {
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.tsx"
    ],
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.test.tsx"
    ],
    "coverageDirectory": "<rootDir>/reports/coverage"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json",
            "README.md",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
